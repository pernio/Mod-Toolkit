{
  "schemaVersion": 1,
  "id": "mod-toolkit",
  "version": "1.0.0",
  "name": "Mod Toolkit",
  "description": "Yet another toolkit for EMC.",
  "authors": ["Crushed_Cookie"],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/emc-mod/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "emcMod.client.EmcModDataGenerator"
    ],
    "client": [
      "emcMod.client.EmcModClient"
    ],
    "main": [
      "emcMod.EmcMod"
    ]
  },
  "mixins": [
    "emc-mod.mixins.json",
    {
      "config": "emc-mod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
